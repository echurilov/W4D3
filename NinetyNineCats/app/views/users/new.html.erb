<form action="<%= users_url  %>" method="post">
<%= hidden_field_tag :authenticity_token, form_authenticity_token -%>

  <h1>Create User</h1>
  
  <br><br>
  
  <label for="username">Username: </label>
  <input
    type="text"
    name="user[username]"
    id="username">
    
  <br><br>
    
  <label for="password">Password: </label>
  <input
    type="password"
    name="user[password]"
    id="password">
    
    <br><br>
    
  <input type="submit" value="Submit">
</form>